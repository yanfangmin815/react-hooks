{
    "presets": ["@babel/react","@babel/env", "react-app"],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-dynamic-import",
        "react-hot-loader/babel",
        "@babel/plugin-proposal-function-bind",
        "@babel/plugin-proposal-optional-chaining",
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css"
        }]
    ]
}
